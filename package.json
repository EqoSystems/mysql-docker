{
  "name": "mysql-docker",
  "version": "8.0.0",
  "docker-registry": "us.gcr.io/qorporate-infrastructure/mysql-8.0-standalone",
  "repository": {
    "url": "https://github.com/EqoSystems/mysql-docker.git"
  },
  "description": "Mysql Server Standalone 8.0",
  "license": "MIT",
  "scripts": {
    "container:auth": "cat qorporate-infrastructure-1f4149d30bf6.json | docker login -u _json_key --password-stdin https://us.gcr.io",
    "container:pull": "npm run container:auth && gcloud components update -q && gcloud auth configure-docker && docker pull mysql/mysql-server:latest",
    "container:push": "npm run container:auth && docker tag mysql:8.0 us.gcr.io/qorporate-infrastructure/mysql-8.0-standalone && docker push us.gcr.io/qorporate-infrastructure/mysql-8.0-standalone",
    "luxx:build:local": "./scripts/setup-mysql-cluster.sh 7.6 build local",
    "luxx:build:beta": "./scripts/setup-mysql-cluster.sh 7.6 build beta",
    "luxx:build:prod": "./scripts/setup-mysql-cluster.sh 7.6 build prod",
    "luxx:deploy:beta": "./scripts/setup-mysql-cluster.sh 7.6 deploy beta",
    "luxx:deploy:prod": "./scripts/setup-mysql-cluster.sh 7.6 deploy prod",
    "luxx:mysql:local": "docker network create luxx-mysql-cluster-local --subnet=10.100.0.0/16",
    "gke:mysql:beta:clean": "gcloud container clusters delete luxx-mysql-beta --quiet",
    "gke:mysql:beta:build": "",
    "gke:mysql:beta:deploy": "",
    "luxx:mysql:prod": "gcloud container clusters create luxx-mysql-prod --network=luxx-northeast-beta --num-nodes=5 --disk-type=pd-ssd --machine-type=n1-standard-1 --services-ipv4-cidr=10.150.2.0/19",
    "luxx:network:local": "docker network create luxx-mysql-cluster-local --subnet=10.100.0.0/16",
    "luxx:network:beta": "docker network create luxx-mysql-cluster-beta cluster --subnet=10.100.0.0/23",
    "luxx:network:prod": "docker network create luxx-mysql-cluster-prod cluster --subnet=10.150.2.0/23",
    "luxx:local:build": "",
    "luxx:local:run": "",
    "luxx:local:deploy": ""
  },
  "dependencies": {
    "@nest-modules/mailer": "^0.2.1",
    "@nestjs/common": "^6.0.5",
    "@nestjs/core": "^6.0.5",
    "@nestjs/microservices": "^6.0.5",
    "@nestjs/platform-express": "^6.0.5",
    "@nestjs/testing": "^6.0.5",
    "@nestjs/typeorm": "^6.0.0",
    "@nestjs/websockets": "^6.0.5",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "docker-build-run-push": "^3.0.0",
    "dotenv": "^5.0.1",
    "googleapis": "^29.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^2.10.4",
    "redis": "^2.8.0",
    "reflect-metadata": "0.1.12",
    "response-time": "^2.3.2",
    "rxjs": "^6.0.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.16",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^2.9.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/node": "^9.6.47",
    "codelyzer": "^5.0.0-beta.2",
    "nodemon": "^1.18.10",
    "ts-node": "^4.1.0"
  }
}
